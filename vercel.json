{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.1"
    }
  },
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/login", "dest": "/api/login.php" },
    { "src": "/register", "dest": "/api/register.php" },
    { "src": "/home", "dest": "/api/home.php" },
    { "src": "/firebase", "dest": "/api/firebase.php" },
    { "src": "/checkout", "dest": "/api/checkout.php" },
    { "src": "/notification", "dest": "/api/notification.php" }
  ]
}
